pipeline {
  agent {
    docker {image 'node:16-alpine'}  
   }
   stages {
      stage('Test') {
        steps {
          sh 'node --version'
        }
      }
   stage('print') {
       steps {
          sh 'echo "node name is $HOSTNAME'
          }
        }
     stage('print') {
       steps {
          sh 'echo "node is installed"'
          }
        }
   }
}
